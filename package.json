  "dependencies": {
    "imagemin": "^5.2.2",
    "imagemin-webp": "^4.0.0",
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^3.3.0",
    "file-loader": "^0.10.1",
